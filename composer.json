{
	"name" : "dtmonterrey/yii2-mailmanager",
	"type" : "yii2-extension",
	"autoload" : {
		"psr-4" : {
			"evandro\\mailmanager\\" : ""
		}
	},
	"minimu-stability" : "stable",
	"require" : {
		"php" : ">=5.4.0",
		"yiisoft/yii2" : "~2.0.5",
		"dtmonterrey/module-user-management" : "^2.0",
		"yiisoft/yii2-swiftmailer" : "~2.0"
	},
	"repositories" : [
		{
			"type": "git",
			"url": "https://github.com/dtmonterrey/user-management",
			"name": "https://github.com/dtmonterrey/user-management"
		}
	],
	"config": {
		"allow-plugins": {
			"yiisoft/yii2-composer": true
		}
	}
}
